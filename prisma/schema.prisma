generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


// creating a new table
model customer {
  id        Int      @id @default(autoincrement())
  // createdAt DateTime @default(now())
  // updatedAt DateTime @updatedAt
  name     String   
  email     String  @unique
  
}

model login {
  user     String @unique
  password String
}
